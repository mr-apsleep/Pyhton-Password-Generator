import random

chars = 'abcdefghijklmnopqrstuvwxyz1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ!@#$'
length = input('password length?')
length = int(length)

number = input ('number of passwords?')
number = int(number)

for p in range(number):
    password = ''
    for c in range(length):
        password += random.choice(chars)
    print(password)
